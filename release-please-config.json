{
  "include-v-in-tag": true,
  "include-component-in-tag": true,
  "release-type": "python",
  "tag-separator": "/",
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "path-expressions": [
        "**",
        "!packages/**"
      ],
      "component": "uvws"
    },
    "packages/core": {
      "changelog-path": "CHANGELOG.md",
      "path-expressions": [
        "packages/core/**"
      ],
      "component": "core"
    },
    "packages/svc1": {
      "changelog-path": "CHANGELOG.md",
      "include-component-in-tag": true,
      "path-expressions": [
        "packages/**"
      ],
      "component": "svc1"
    }
  },
  "plugins": [
    {
      "type": "linked-versions",
      "groupName": "my group",
      "components": [
        "uvws", "core", "svc1"
      ]
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
